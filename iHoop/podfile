# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'iHoop' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Firebase Pods for iHoop
  pod 'Firebase/Core'
  pod 'Firebase/Storage'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FBSDKLoginKit'
  
  # Google Pods for iHoop
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'

  target 'iHoopTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iHoopUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
